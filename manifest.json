{
  "manifest_version": 2,
  "name": "NEWS HEADLINES",
  "version": "1.0",
  "description": "Latest News on the go!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "https://newsapi.org/*"
  ],
  "content_security_policy": "script-src 'self' https://newsapi.org/; object-src 'self'"
}
